<?xml version="1.0" encoding="utf-8"?>
<header name="boost/compute/container/basic_string.hpp">
  <namespace name="boost">
    <namespace name="compute">

      <!-- boost::compute::basic_string<CharT, Traits> -->
      <class name="basic_string">
        <purpose>A string of characters</purpose>

        <template>
          <template-type-parameter name="CharT">
            <purpose>character type</purpose>
          </template-type-parameter>
          <template-type-parameter name="Traits">
            <default>std::char_traits&lt;CharT&gt;</default>
            <purpose>character traits</purpose>
          </template-type-parameter>
        </template>

        <description>
          <para>
            See also: <classname>string</classname>
          </para>
        </description>
      </class>
    </namespace>
  </namespace>
</header>
