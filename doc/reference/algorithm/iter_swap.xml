<?xml version="1.0" encoding="utf-8"?>
<header name="boost/compute/algorithm/iter_swap.hpp">
  <namespace name="boost">
    <namespace name="compute">

      <!-- boost::compute::iter_swap(a, b, queue) -->
      <function name="iter_swap">
        <template>
          <template-type-parameter name="Iterator1"/>
          <template-type-parameter name="Iterator2"/>
        </template>

        <type>void</type>

        <parameter name="a">
          <paramtype>Iterator1</paramtype>
        </parameter>
        <parameter name="b">
          <paramtype>Iterator2</paramtype>
        </parameter>
        <parameter name="queue">
          <paramtype>command_queue &amp;</paramtype>
        </parameter>

        <description>
          <para>
            Swaps the elements pointed to by the the iterators <code>a</code>
            and <code>b</code>.
          </para>
        </description>
      </function>
    </namespace>
  </namespace>
</header>
