<?xml version="1.0" encoding="utf-8"?>
<header name="boost/compute/algorithm/generate_n.hpp">
  <namespace name="boost">
    <namespace name="compute">

      <!-- boost::compute::generate_n(first, count, generator, queue) -->
      <function name="generate_n">
        <template>
          <template-type-parameter name="OutputIterator"/>
          <template-type-parameter name="Size"/>
          <template-type-parameter name="Generator"/>
        </template>

        <type>void</type>

        <parameter name="first">
          <paramtype>OutputIterator</paramtype>
        </parameter>
        <parameter name="count">
          <paramtype>Size</paramtype>
        </parameter>
        <parameter name="generator">
          <paramtype>Generator</paramtype>
        </parameter>
        <parameter name="queue">
          <paramtype>command_queue &amp;</paramtype>
        </parameter>

        <description>
          <para>
            Stores the result of <code>generator</code> for each element in
            the range [<code>first</code>, <code>first + count</code>).
          </para>
          <para>
            See also: <functionname>generate()</functionname>
          </para>
        </description>
      </function>
    </namespace>
  </namespace>
</header>
